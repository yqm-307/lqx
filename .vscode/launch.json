    "version": "0.2.0",
    "configurations": [
        {
            "name": "service monitor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/monitor/monitor",
            "args": ["-c", "${workspaceFolder}/project/monitor/config.ini"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/monitor",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "service database",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/database/database",
            "args": ["-c", "${workspaceFolder}/project/database/config.ini"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/database",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "test_database",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/database/test/test_database",
            "args": ["--log_level=all"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}